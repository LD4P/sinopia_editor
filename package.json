{
  "name": "sinopia_editor",
  "description": "Editor for BIBFRAME data forked from http://github.com/lcnetdev/bfe",
  "keywords": [
    "bibframe",
    "editor",
    "rdf"
  ],
  "version": "3.12.0",
  "homepage": "http://github.com/LD4P/sinopia_editor/",
  "repository": {
    "type": "git",
    "url": "https://github.com/LD4P/sinopia_editor.git"
  },
  "engines": {
    "node": ">= 14.11.0"
  },
  "contributors": [
    "Kirk Hess <khes@loc.gov>",
    "Jeremy Nelson <jpnelson@stanford.edu>",
    "Joshua Greben",
    "Naomi Dushay",
    "Sarav Shah",
    "Johnathan Martin",
    "Michael Giarlo",
    "Justin Coyne",
    "Peter Mangiafico",
    "Justin Littman",
    "Aaron Collier",
    "Huda Khan"
  ],
  "bugs": {
    "url": "http://github.com/LD4P/sinopia_editor/issues"
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "webpack --progress --mode production",
    "dev-build": "webpack --progress --mode development",
    "dev-build-test": "npm run dev-build && npm run test",
    "dev-start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "eslint": "eslint --color -c .eslintrc.js --ext js,jsx --max-warnings 0 .",
    "eslint-fix": "eslint --color -c .eslintrc.js --ext js,jsx --fix .",
    "eslint-fix-single": "eslint --color -c .eslintrc.js --fix",
    "pretty": "prettier --check .",
    "pretty-fix": "prettier --write .",
    "fix": "npm run pretty-fix && npm run eslint-fix",
    "lint": "npm run pretty && npm run eslint",
    "start": "npx babel-node server.js",
    "test": "jest --colors --silent --runInBand",
    "test-verbose": "jest --colors --runInBand",
    "cypress-verify": "cypress verify",
    "cypress-open": "cypress open",
    "cypress-run": "cypress run -b chrome --headless"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-amplify/auth": "^4.3.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@honeybadger-io/react": "^1.0.1",
    "@popperjs/core": "^2.10.1",
    "@rdfjs/serializer-jsonld-ext": "^2.0.0",
    "bootstrap": "^5.1.1",
    "buffer": "^6.0.3",
    "concat-stream": "^2.0.0",
    "cors": "^2.8.5",
    "crypto-browserify": "3.12.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^8.6.0",
    "event-stream": "^4.0.1",
    "express": "^4.16.4",
    "file-saver": "^2.0.5",
    "html-webpack-plugin": "^5.3.2",
    "identity-obj-proxy": "^3.0.0",
    "javascript-time-ago": "^2.3.9",
    "jsonld-streaming-parser": "^2.4.0",
    "lodash": "^4.17.21",
    "merge": "^2.1.1",
    "mixin-deep": ">=1.3.2",
    "n3": "^1.11.1",
    "nanoid": "^3.1.25",
    "process": "^0.11.10",
    "rdf-ext": "^1.3.5",
    "react": "^16.14.0",
    "react-bootstrap-icons": "^1.5.0",
    "react-bootstrap-typeahead": "^5.2.0",
    "react-dom": "^16.14.0",
    "react-helmet-async": "^1.1.2",
    "react-hot-loader": "^4.13.0",
    "react-multi-select-component": "^4.1.11",
    "react-offcanvas": "^0.4.0",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-textarea-autosize": "^8.3.3",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "stream-browserify": "^3.0.0",
    "swagger-client": "^3.16.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/node": "^7.15.4",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.15.4",
    "@testing-library/dom": "^8.2.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "autoprefixer": "^10.3.4",
    "babel-jest": "^27.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "css-loader": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "~14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "fetch-mock-jest": "^1.5.1",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.1.0",
    "jest-localstorage-mock": "^2.4.17",
    "jest-mock-console": "^1.1.0",
    "jest-prop-type-error": "^1.1.0",
    "jest-summarizing-reporter": "^1.1.4",
    "jsdom": "^17.0.0",
    "jsdom-global": "3.0.2",
    "postcss-loader": "^6.1.1",
    "prettier": "2.4.0",
    "raw-loader": "^4.0.2",
    "redux-mock-store": "^1.5.3",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.1"
  },
  "jest": {
    "automock": false,
    "resetMocks": false,
    "setupFilesAfterEnv": [
      "<rootDir>/react-testing-library.setup.js",
      "<rootDir>/__tests__/matchers/templates.js",
      "<rootDir>/__tests__/matchers/resources.js",
      "<rootDir>/__tests__/matchers/actions.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "__tests__/testUtilities"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|sass|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/**/*.jsx",
      "!src/index.js"
    ],
    "setupFiles": [
      "jest-localstorage-mock",
      "jest-prop-type-error",
      "dotenv/config"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/__tests__/**/*.test.[jt]s?(x)"
    ],
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    },
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "reporters": [
      [
        "jest-summarizing-reporter",
        {
          "diffs": true
        }
      ]
    ]
  },
  "optionalDependencies": {
    "cypress": "^8.3.1"
  }
}
