{
  "name": "sinopia_editor",
  "description": "Editor for BIBFRAME data forked from http://github.com/lcnetdev/bfe",
  "keywords": [
    "bibframe",
    "editor",
    "rdf"
  ],
  "version": "3.1.0",
  "homepage": "http://github.com/LD4P/sinopia_editor/",
  "repository": {
    "type": "git",
    "url": "https://github.com/LD4P/sinopia_editor.git"
  },
  "engines": {
    "node": ">= 11.0.0"
  },
  "contributors": [
    "Kirk Hess <khes@loc.gov>",
    "Jeremy Nelson <jpnelson@stanford.edu>",
    "Joshua Greben",
    "Naomi Dushay",
    "Sarav Shah",
    "Johnathan Martin",
    "Michael Giarlo",
    "Justin Coyne",
    "Peter Mangiafico",
    "Justin Littman",
    "Aaron Collier",
    "Huda Khan"
  ],
  "bugs": {
    "url": "http://github.com/LD4P/sinopia_editor/issues"
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "webpack -p --progress --mode production",
    "dev-build": "webpack --progress --mode development",
    "dev-build-test": "npm run dev-build && npm run test",
    "dev-start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "eslint": "eslint --color -c .eslintrc.js --ext js,jsx --max-warnings 0 .",
    "eslint-fix": "eslint --color -c .eslintrc.js --ext js,jsx --fix .",
    "eslint-fix-single": "eslint --color -c .eslintrc.js --fix",
    "jest-cov": "jest --coverage --colors --silent --runInBand",
    "jest-ci": "jest  --colors --silent --ci --runInBand --coverage --reporters=default --reporters=jest-junit && cat ./coverage/lcov.info | coveralls",
    "start": "npx babel-node server.js",
    "test": "jest --colors --silent --detectOpenHandles",
    "test-verbose": "jest --colors --runInBand",
    "cypress-verify": "cypress verify",
    "cypress-open": "cypress open",
    "cypress-run": "cypress run -b chrome --headless"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-amplify/auth": "^3.4.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@honeybadger-io/react": "0.0.6",
    "@rdfjs/serializer-jsonld-ext": "^2.0.0",
    "ajv": "^6.12.4",
    "bootstrap": "^4.5.2",
    "concat-stream": "^2.0.0",
    "cors": "^2.8.5",
    "crypto-js": "^3.3.0",
    "dotenv": "^8.0.0",
    "eslint-utils": "^2.1.0",
    "event-stream": "^4.0.1",
    "express": "^4.16.4",
    "file-saver": "^2.0.2",
    "hookrouter": "^1.2.3",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jquery": "^3.5.1",
    "jsonld-streaming-parser": "^2.0.2",
    "lodash": "^4.17.20",
    "merge": "^1.2.1",
    "mixin-deep": ">=1.3.2",
    "n3": "^1.6.2",
    "popper.js": "^1.16.1",
    "query-string": "^6.13.1",
    "rdf-ext": "^1.3.0",
    "react": "^16.13.1",
    "react-bootstrap-typeahead": "^3.4.2",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.6",
    "react-hot-loader": "^4.12.21",
    "react-offcanvas": "^0.4.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-textarea-autosize": "^8.2.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "set-value": "^3.0.2",
    "shortid": "^2.2.14",
    "swagger-client": "^3.10.12",
    "uuid": "^8.3.0",
    "x2js": "^3.2.6"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@testing-library/dom": "^7.24.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "coveralls": "^3.1.0",
    "css-loader": "^3.6.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "~14.0.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-security": "^1.4.0",
    "file-loader": "^5.1.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^26.4.2",
    "jest-junit": "^10.0.0",
    "jest-localstorage-mock": "^2.4.3",
    "jest-mock-console": "^1.0.1",
    "jest-prop-type-error": "^1.1.0",
    "jsdom": "^16.4.0",
    "jsdom-global": "3.0.2",
    "node-sass": "^4.14.1",
    "raw-loader": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/setupEnzyme.js",
      "<rootDir>/react-testing-library.setup.js",
      "<rootDir>/__tests__/matchers/templates.js",
      "<rootDir>/__tests__/matchers/resources.js",
      "<rootDir>/__tests__/matchers/actions.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "__tests__/testUtilities"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|sass|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/**/*.jsx",
      "!src/index.js"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupFiles": [
      "jest-localstorage-mock",
      "jest-prop-type-error",
      "dotenv/config",
      "<rootDir>/test-env.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.[jt]s?(x)"
    ],
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    }
  },
  "optionalDependencies": {
    "cypress": "^5.1.0"
  }
}
